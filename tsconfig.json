{
  "compilerOptions": {
    "target": "es2021",
    "module": "es2020",
    "moduleResolution": "node",
    "lib": ["es2021", "es2017", "dom"],
    "jsx": "react",
    "declaration": true,
    "strict": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "sourceMap": true
  }
}
