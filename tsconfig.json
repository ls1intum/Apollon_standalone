{
  "compilerOptions": {
    "target": "es2024",
    "module": "preserve",
    "moduleResolution": "node",
    "lib": ["es2022", "dom"],
    "jsx": "react",
    "declaration": true,
    "strict": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "downlevelIteration": true,
    "resolveJsonModule": true,
    "sourceMap": true
  }
}
