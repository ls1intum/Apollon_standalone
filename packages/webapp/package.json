{
  "name": "webapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf ../../build/webapp",
    "start": "webpack serve --config ./webpack/webpack.dev.js",
    "lint": "eslint -c .eslintrc.js \"**/*.{ts,tsx}\"",
    "lint:ts": "eslint -c .eslintrc.js \"/src/main\"",
    "lint:css": "stylelint \"src/**/*.ts\"",
    "build": "npm run clean && webpack --config ./webpack/webpack.prod.js",
    "build:local": "npm run clean && DEPLOYMENT_URL=http://localhost:8080 webpack --config ./webpack/webpack.prod.js"
  },
  "engines": {
    "node": ">=22.10.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.2.7",
    "@restart/hooks": "^0.4.16",
    "@restart/ui": "^1.9.4",
    "@sentry/react": "8.38.0",
    "bootstrap": "5.3.3",
    "classnames": "^2.5.1",
    "dom-helpers": "^5.2.1",
    "es5-ext": "^0.10.64",
    "html-minifier-terser": "^6.1.0",
    "invariant": "^2.2.4",
    "posthog-js": "1.186.3",
    "prop-types-extra": "^1.1.1",
    "react": "18.2.0",
    "react-bootstrap": "2.10.5",
    "react-bootstrap-icons": "1.11.4",
    "react-dom": "18.2.0",
    "react-redux": "8.1.3",
    "react-router-dom": "6.22.3",
    "react-transition-group": "^4.4.5",
    "shared": "*",
    "styled-components": "6.1.13",
    "stylis": "4.3.4",
    "uncontrollable": "^7.2.1",
    "uuid": "9.0.1",
    "websocket": "1.0.35"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "0.38.0",
    "@svgr/webpack": "8.1.0",
    "@types/react": "18.2.79",
    "@types/react-bootstrap": "0.32.37",
    "@types/react-dom": "18.2.25",
    "@types/react-redux": "^7.1.34",
    "@types/styled-components": "5.1.34",
    "@types/uuid": "10.0.0",
    "@types/websocket": "^1.0.10",
    "circular-dependency-plugin": "5.2.2",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.11.0",
    "ejs-loader": "0.5.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "html-webpack-plugin": "5.6.3",
    "react-toastify": "9.1.3",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.10.0"
  }
}
