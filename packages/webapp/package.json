{
  "name": "webapp",
  "version": "2.4.2",
  "private": true,
  "scripts": {
    "clean": "rm -rf ../../build/webapp",
    "start": "webpack serve --config webpack/webpack.dev.cjs",
    "lint": "eslint -c .eslintrc.js \"**/*.{ts,tsx}\"",
    "lint:ts": "eslint -c .eslintrc.js \"/src/main\"",
    "lint:css": "stylelint \"src/**/*.ts\"",
    "build": "npm run clean && webpack --config webpack/webpack.prod.cjs",
    "build:local": "npm run clean && DEPLOYMENT_URL=http://localhost:8080 webpack --config webpack/webpack.prod.cjs"
  },
  "type": "module",
  "engines": {
    "node": ">=24.7.0",
    "npm": ">=11.5.1"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Edge versions",
    "Safari >= 16.4",
    "iOS >= 16.4"
  ],
  "overrides": {
    "semver": "7.7.3",
    "word-wrap": "1.2.5",
    "tough-cookie": "6.0.0",
    "postcss": "8.5.6",
    "crypto-js": "4.2.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.9.2",
    "bootstrap": "5.3.8",
    "posthog-js": "1.284.0",
    "react": "18.3.1",
    "react-bootstrap": "2.10.10",
    "react-bootstrap-icons": "1.11.6",
    "react-dom": "18.3.1",
    "react-redux": "8.1.3",
    "react-router-dom": "6.22.3",
    "shared": "*",
    "styled-components": "6.1.19",
    "stylis": "4.3.6",
    "uuid": "13.0.0",
    "websocket": "1.0.35"
  },
  "devDependencies": {
    "@stylelint/postcss-css-in-js": "0.38.0",
    "@svgr/webpack": "8.1.0",
    "@types/react": "18.3.26",
    "@types/react-dom": "18.3.7",
    "@types/react-redux": "7.1.34",
    "@types/styled-components": "5.1.36",
    "@types/websocket": "1.0.10",
    "circular-dependency-plugin": "5.2.2",
    "copy-webpack-plugin": "13.0.1",
    "css-loader": "6.11.0",
    "ejs-loader": "0.5.0",
    "fork-ts-checker-webpack-plugin": "9.1.0",
    "html-webpack-plugin": "5.6.4",
    "react-toastify": "9.1.3",
    "stylelint-config-styled-components": "0.1.1"
  }
}
