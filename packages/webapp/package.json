{
  "name": "webapp",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf ../../build/webapp",
    "start": "webpack-dev-server --config ./webpack/webpack.dev.js",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:ts": "tslint --force -p tsconfig.json -c tslint.json -t stylish",
    "lint:css": "stylelint 'src/**/*.ts'",
    "build": "yarn clean && webpack --config ./webpack/webpack.prod.js",
    "build:local": "yarn clean && DEPLOYMENT_URL=http://localhost:8080 webpack --config ./webpack/webpack.prod.js"
  },
  "engines": {
    "node": ">=14.1.0",
    "yarn": ">=1.22.5"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@ls1intum/apollon": "2.4.0",
    "bootstrap": "4.5.3",
    "moment": "2.29.1",
    "react": "17.0.1",
    "react-bootstrap": "1.4.0",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-observable": "1.2.0",
    "rxjs": "6.6.3",
    "shared": "1.0.1",
    "styled-components": "5.2.1",
    "tslib": "2.0.3",
    "typesafe-actions": "5.1.0",
    "uuid": "8.3.2",
    "websocket": "^1.0.33"
  },
  "devDependencies": {
    "@types/react": "17.0.0",
    "@types/react-bootstrap": "1.0.1",
    "@types/react-dom": "17.0.0",
    "@types/react-redux": "7.1.15",
    "@types/react-router-dom": "5.1.7",
    "@types/styled-components": "5.1.7",
    "@types/uuid": "8.3.0",
    "circular-dependency-plugin": "5.2.2",
    "copy-webpack-plugin": "6.4.0",
    "css-loader": "5.0.1",
    "ejs-loader": "0.5.0",
    "fork-ts-checker-notifier-webpack-plugin": "3.0.0",
    "fork-ts-checker-webpack-plugin": "6.0.8",
    "html-webpack-plugin": "4.5.1",
    "prettier": "2.2.1",
    "style-loader": "2.0.0",
    "stylelint": "13.8.0",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-custom-processor-loader": "0.6.0",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-loader": "8.0.13",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-loader": "3.5.4",
    "tslint-react": "5.0.0",
    "typescript": "4.1.3",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.1",
    "webpack-merge": "5.2.0"
  }
}
