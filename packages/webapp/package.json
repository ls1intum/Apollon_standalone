{
  "name": "webapp",
  "version": "1.6.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf ../../build/webapp",
    "start": "webpack serve --config ./webpack/webpack.dev.js",
    "lint": "yarn lint:ts && yarn lint:css",
    "lint:ts": "tslint --force -p tsconfig.json -c tslint.json -t stylish",
    "lint:css": "stylelint 'src/**/*.ts'",
    "build": "yarn clean && webpack --config ./webpack/webpack.prod.js",
    "build:local": "yarn clean && DEPLOYMENT_URL=http://localhost:8080 webpack --config ./webpack/webpack.prod.js"
  },
  "engines": {
    "node": ">=14.1.0",
    "yarn": ">=1.22.0"
  },
  "dependencies": {
    "@ls1intum/apollon": "2.12.2",
    "bootstrap": "5.1.3",
    "moment": "2.29.3",
    "react": "17.0.2",
    "react-bootstrap": "1.6.4",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router-dom": "5.3.0",
    "redux": "4.1.2",
    "redux-observable": "2.0.0",
    "rxjs": "7.3.0",
    "shared": "1.6.1",
    "styled-components": "5.3.5",
    "tslib": "2.4.0",
    "typesafe-actions": "5.1.0",
    "uuid": "8.3.2",
    "websocket": "1.0.34"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.2.1",
    "@types/react": "17.0.37",
    "@types/react-bootstrap": "0.32.30",
    "@types/react-dom": "17.0.11",
    "@types/react-redux": "7.1.24",
    "@types/react-router-dom": "5.3.3",
    "@types/styled-components": "5.1.25",
    "@types/uuid": "8.3.4",
    "circular-dependency-plugin": "5.2.2",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.7.1",
    "ejs-loader": "0.5.0",
    "fork-ts-checker-notifier-webpack-plugin": "6.0.0",
    "fork-ts-checker-webpack-plugin": "7.2.11",
    "html-webpack-plugin": "5.5.0",
    "prettier": "2.7.1",
    "react-toastify": "^9.0.5",
    "style-loader": "3.3.1",
    "stylelint": "14.9.1",
    "stylelint-config-recommended": "8.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-custom-processor-loader": "0.6.0",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-loader": "9.3.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "tslint-loader": "3.5.4",
    "tslint-react": "5.0.0",
    "typescript": "4.7.4",
    "webpack": "5.73.0",
    "webpack-cli": "4.10.0",
    "webpack-dev-server": "4.9.3",
    "webpack-merge": "5.8.0"
  }
}
